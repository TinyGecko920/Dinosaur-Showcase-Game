          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const tm = runtimeScene.getTimeManager();",
            "",
            "if (!tm.hasTimer('transitionTimer')) {",
            "    tm.addTimer('transitionTimer');",
            "}",
            "",
            "const tp = objects[0];",
            "",
            "function transFunc(x) {",
            "    return Math.pow(x, 7) - (1.3 * Math.pow(x, 2)) + x;",
            "}",
            "",
            "tp.getVariables().get('OutlineSize').setNumber(",
            "    (1 - transFunc(tm.getTimer('transitionTimer').getTime() / 800)) * runtimeScene.getViewportWidth()",
            ");",
            "",
            "if (tp.getVariables().get('OutlineSize').getAsNumber() <= 0) {",
            "    runtimeScene.getVariables().get('Transitioning').setBoolean(false);",
            "}"
          ],
          "parameterObjects": "TransitionPainter",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
